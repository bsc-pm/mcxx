# This file controls how external programs are handled
[lt-mcxx]
language = C
options =
preprocessor_name = gcc
preprocessor_options = -E -I@NANOS_INCLUDES@ -D_OPENMP=200505 -include omp.h -include nth_mcc.h
compiler_name = gcc
compiler_options = 
linker_name = gcc
linker_options = -L@NANOS_LIBS@ -Wl,-rpath,@NANOS_LIBS@
pragma_prefix = nanos
compiler_phase = libtlnanos.so
compiler_phase = libtlomp-core.so
compiler_phase = libtlomp.so
